{
    "root": true,
    "extends": "airbnb-base",
    "env": {
      "browser": true,
      "es2021": true
    },
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "no-param-reassign": [2, { "props": false }],
      "linebreak-style": ["error", "unix"],
      "import/extensions": ["error", "always"]
    }
  }
  