{
    "@context": "https://schema.org",
    "@type": "Event",
    "name": "$meta:eventname",
    "startDate": "$meta:eventstartdate$",
    "endDate": "$meta:eventenddate$",
    "location": {
      "@type": "Place",
      "name": "$meta:eventlocationname$",
      "address": {
        "@type": "PostalAddress",
        "streetAddress": "$meta:eventstreetaddress$",
        "addressLocality": "$meta:eventcity$",
        "addressRegion": "$meta:eventregion$",
        "postalCode": "$meta:eventpostalcode$"
      }
    },
    "image": "$event:image$",
    "description": "$event:description$",
    "offers": {
      "@type": "Offer",
      "price": "$meta:price$",
      "priceCurrency": "$meta:pricecurrency$",
      "availability": "https://schema.org/InStock",
      "url": "$meta:eventsignupurl$"
    }
  }
  